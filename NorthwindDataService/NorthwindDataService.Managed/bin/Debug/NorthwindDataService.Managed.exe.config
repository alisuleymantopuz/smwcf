<?xml version="1.0"?>
<configuration>
    <startup>
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="NorthwindDataService.Managed.Implementations.ProductService" behaviorConfiguration="ProductServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:9001/ProductService"/>
            <add baseAddress="net.tcp://localhost:9002/ProductService"/>
          </baseAddresses>
        </host>
        <endpoint address="http://localhost:9001/ProductService" binding="basicHttpBinding" contract="NorthwindDataService.Managed.Contracts.IProductService"/>
        <endpoint address="net.tcp://localhost:9002/ProductService" binding="netTcpBinding" contract="NorthwindDataService.Managed.Contracts.IProductService"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="ProductServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="False"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>